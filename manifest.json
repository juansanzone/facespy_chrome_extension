{
	"name": "FaceSpy Beta",
	"description": "Chrome extension to see photos of any Facebook User",
	"version": "1.0",
	"manifest_version": 2,
  	"permissions": [
  		"tabs",
  		"https://ajax.googleapis.com/",
    	"cookies",
			"<all_urls>"
  	],
  	"browser_action": {
     	"default_icon": "assets/256.png",
     	"default_popup": "fbackup.html"
  	},
 	"icons": {
        "16": "assets/16.png",
        "48": "assets/48.png",
        "128": "assets/128.png",
        "256": "assets/256.png"
    }
}
